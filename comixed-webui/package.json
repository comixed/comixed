  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format": "node_modules/.bin/prettier --write 'src/app/**/*.{ts,json,html,scss}'"
  },
  "prettier": {
    "singleQuote": true,
    "useTabs": false,
    "bracketSpacing": true,
    "trailingComma": "none",
    "jsxBracketSameLine": true,
    "arrowParens": "avoid",
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular",
          "singleQuote": false,
          "jsxBracketSameLine": false,
          "htmlWhitespaceSensitivity": "ignore"
        }
      }
    ]
  },
